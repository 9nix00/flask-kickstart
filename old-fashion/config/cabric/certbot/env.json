{
  "dns-list": [
    {
      "isp": "dnspod.cn",
      "domain": "kay8.com",
      "records": [
        {
          "value": "___ROUTER-IP___",
          "sub_domain": "___pkg___",
          "record_type": "A"
        }
      ]
    }
  ],
  "ssl": {
    "domains": [
      "api.___DOMAIN___"
    ],
    "dhparam": {
      "path": "/etc/ssl/certs/dhparam.pem",
      "length": 4096
    },
    "load-balancer": {
      "isp": "qingcloud.com",
      "zone": "___CLOUD-ZONE___",
      "id": "___LB-ID___",
      "http-listener": "___HTTP-LISTENER___",
      "backend": {
        "resource_id": "___LB-BACKEND-ID___",
        "port": 81
      }
    },
    "encrypt-position": 0
  }
}